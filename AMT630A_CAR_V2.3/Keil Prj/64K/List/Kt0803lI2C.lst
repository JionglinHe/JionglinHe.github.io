              ************************************************************/
              FLAG Kt0803lACKCheck(void)
              {
                  UCHAR loop = 200;
                      
                  SetSdaInput();
                      
                      DelayKt0803lI2c(2);                                                                
                      SetScl(1);
                      do
                      {
                          DelayKt0803lI2c(2);
                              if(GetSda() == 0)
                              {
                                 SetScl(0);                                                                      
                             return(0);    //这里说明有ACK信号  
                              }
                              else
                              {
                                 if(loop == 1)
                                 {
                                      SetScl(0);                                                                         
                                  return(1);   
                                 }
                              }
                      }while(--loop) ;
                  return 1;   
              }
              
              
