              void Kt0803lI2CStart(void)
              {  
                      SetSdaOutput();
                  SetSclOutput();
              
                      DelayKt0803lI2c(5);
                                                              //___
                      SetSda(1);                      //       |
                      SetScl(1);                      //      |___SDA
