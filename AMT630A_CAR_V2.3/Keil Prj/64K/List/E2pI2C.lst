              /***********************************************************
              *name:    I2CWrite(UCHAR tempdata)
              *input:   UCHAR tempdata
              *output:  对相应的空间写一个字节数据
              *update:  2009-03-25
              
              *description:   iic的write一个字节函数 
                              100u
              *history:
              ************************************************************/
              void E2pI2CWrite(UCHAR tempdata)
              {
                      UCHAR XDATA num;                           //"发送数据位数"
                      
                      SetSdaOutput();
                      
                      for(num = 0;num < 8;num ++)
                      {
                              SetSda((tempdata >> (7-num)) & 1);      //"先发高位"
                              DelayE2pI2c(2);
                              SetScl(1);                        //下降缘 读取
                              DelayE2pI2c(2);
                              SetScl(0);
                              DelayE2pI2c(2); 
                      }
                      SetSda(1);
              }
              
              
              /***********************************************************
              *name:    I2CRead(void)
              *input:   non
              *output:  返回读出的字节
              *update:  2009-03-25
              
              *description:   iic的read一个字节 数据函数
              *history:
              ************************************************************/
              UCHAR E2pI2CRead(void)
              {
              
                      UCHAR XDATA temp = 0xff;
                      UCHAR XDATA num;         //"接收数据位数"
              
                      SetSdaInput();
              
                      for(num = 0;num < 8;num ++)
                      {
                        
                              SetScl(1);
                              DelayE2pI2c(2);
                              temp = temp << 1;//"先接收高位"
              
                              if(GetSda() == 1)
                              {
                                      temp = temp | 0x01;
C51 COMPILER V9.00   E2PI2C                                                                06/07/2020 11:05:35 PAGE 5   

                              }
              
                              if(GetSda() == 0)
                              {
                                      temp = temp & 0xfe;
                              }
              
                              SetScl(0);
                              DelayE2pI2c(2);
                      }
              
                      return (temp);
              }
